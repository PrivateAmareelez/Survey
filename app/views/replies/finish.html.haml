.container-fluid
  .row
    .col-xs-2
    .col-xs-5
      = image_tag image_path('LOGO.gif'), class: "img-responsive"
    .col-xs-1
    .col-xs-2
      = image_tag image_path('logo_ts.gif'), class: "img-responsive"
    .col-xs-2
.panel.panel-default
  .panel-body
    -if flash[:error]
      .alert.alert-danger
        = flash[:error]
    -if flash[:success]
      .alert.alert-success
        = flash[:success]